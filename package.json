{
  "name": "trauko.neocities.org",
  "description": "My personal website on Neocities, built with 11ty.",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch src/scss:dist/assets/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass --no-source-map src/scss:dist/assets/css",
    "build:eleventy": "eleventy",
    "dev": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chileangoblin/trauko.neocities.org.git"
  },
  "author": "chileangoblin",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@11ty/eleventy": "3.1.1",
    "npm-run-all": "4.1.5",
    "sass": "1.89.2"
  }
}